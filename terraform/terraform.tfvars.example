# Example Terraform variables file
# Copy this file to terraform.tfvars and fill in your actual values
# DO NOT commit terraform.tfvars to version control

# Kubernetes configuration
kubeconfig = "~/.kube/config"

# Environment configuration
environment = "eksa-dev"
target_revision = "master"

# Wazuh Helm repository credentials
# Replace with your actual Wazuh Helm repository credentials
wazuh_helm_username = "your-wazuh-helm-username"
wazuh_helm_password = "your-wazuh-helm-password"

# Certificate manager configuration
# Replace with your actual email address for Let's Encrypt notifications
cert_manager_email = "your-email@example.com"